{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task/api:/var/task"
  },
  "crons": [
    {
      "path": "/api/attendance/process-auto-absent",
      "schedule": "*/5 * * * *"
    }
  ]
}
